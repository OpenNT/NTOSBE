TARGETNAME=nosrvbld
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=$(WINDOWS_INC_PATH)

SOURCES=nosrvbld.c

MSC_WARNING_LEVEL=/W3 /WX

C_DEFINES=-D_OS2_20_=0 -Dnear= -Dfar= -Dpascal= -Dstricmp=_stricmp

UMTYPE=console
UMLIBS=$(SDK_LIB_PATH)\kernl32p.lib

USE_MSVCRT=1
